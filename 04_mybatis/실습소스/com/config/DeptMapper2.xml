<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.config.DeptMapper2">

      <!-- 전체조회 -->
    <select id="findAll" resultType="DeptDTO">
    	select deptno, dname, loc
    	from dept
    	order by deptno asc
    </select>
   
   
</mapper>


